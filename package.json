{
  "name": "x3-parser-csv",
  "version": "1.0.0",
  "description": "A really fast streaming CSV parser written in Typescript.",
  "repository": {
    "type": "git",
    "url": "https://github.com/x3cion/x3-parser-csv.git"
  },
  "author": "x3cion",
  "main": "dist/parser.js",
  "scripts": {
    "test__description": "echo 'Uses find instead of glob pattern for Node 18 compatibility (Node 18 --test does not support ** globs)'",
    "test": "c8 node --import tsx --test $(find src -name '*.spec.ts')",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "prepublishOnly": "rm -rf dist/ && tsc",
    "prepare": "husky || true"
  },
  "type": "module",
  "typings": "dist/parser.d.ts",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@types/node": "^22.0.0",
    "c8": "^10.1.3",
    "csv-spectrum": "github:x3cion/csv-spectrum#fix/location_coordinates",
    "eslint": "^9.18.0",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "prettier": "^3.4.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.18.2",
    "zod": "^4.1.11"
  },
  "packageManager": "pnpm@10.19.0+sha512.c9fc7236e92adf5c8af42fd5bf1612df99c2ceb62f27047032f4720b33f8eacdde311865e91c411f2774f618d82f320808ecb51718bfa82c060c4ba7c76a32b8"
}
