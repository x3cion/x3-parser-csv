{
  "name": "x3-parser-csv",
  "version": "0.1.4",
  "description": "A really fast streaming CSV parser written in Typescript.",
  "repository": {
    "type": "git",
    "url": "https://github.com/x3cion/x3-parser-csv.git"
  },
  "author": "x3cion",
  "main": "dist/parser.js",
  "scripts": {
    "test__description": "echo 'Uses find instead of glob pattern for Node 18 compatibility (Node 18 --test does not support ** globs)'",
    "test": "c8 node --import tsx --test $(find src -name '*.spec.ts')",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "prepublishOnly": "rm -rf dist/ && tsc",
    "prepare": "husky || true"
  },
  "type": "module",
  "typings": "dist/parser.d.ts",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@types/node": "^22.0.0",
    "c8": "^10.1.3",
    "csv-spectrum": "github:x3cion/csv-spectrum#fix/location_coordinates",
    "eslint": "^9.18.0",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "prettier": "^3.4.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.18.2",
    "zod": "^4.1.11"
  },
  "packageManager": "pnpm@10.18.0+sha512.e804f889f1cecc40d572db084eec3e4881739f8dec69c0ff10d2d1beff9a4e309383ba27b5b750059d7f4c149535b6cd0d2cb1ed3aeb739239a4284a68f40cfa"
}
