{
  "name": "x3-parser-csv",
  "version": "0.1.4",
  "description": "A really fast streaming CSV parser written in Typescript.",
  "repository": {
    "type": "git",
    "url": "https://github.com/x3cion/x3-parser-csv.git"
  },
  "author": "x3cion",
  "main": "dist/parser.js",
  "scripts": {
    "test": "cross-env TS_NODE_COMPILER_OPTIONS={\\\"module\\\":\\\"commonjs\\\"} nyc mocha -r ts-node/register -r source-map-support/register --recursive src/**/*.spec.ts --delay",
    "prepublishOnly": "rm -rf dist/ && tsc"
  },
  "typings": "dist/parser.d.ts",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "~4.3.0",
    "@types/mocha": "~5.0.0",
    "@types/node": "~9.6.5",
    "chai": "~4.5.0",
    "cross-env": "~5.1.4",
    "csv-spectrum": "~1.0.0",
    "mocha": "~5.1.0",
    "nyc": "14.1.1",
    "source-map-support": "~0.5.4",
    "ts-node": "~6.0.0",
    "tslint": "5.18.0",
    "typescript": "~2.8.1"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts",
      "**/*.spec.ts"
    ],
    "reporter": [
      "html",
      "text"
    ],
    "include": [
      "src/**/*.ts"
    ]
  }
}
